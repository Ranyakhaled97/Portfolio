 <!-- ======= Intro Section ======= -->
@{ 
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<main>
<section id="intro">
    <div class="intro-container">
        <div id="introCarousel" class="carousel  slide carousel-fade" data-ride="carousel">

            <ol class="carousel-indicators"></ol>

            <div class="carousel-inner" role="listbox">

                <div class="carousel-item active">
                    <img src="~/assets/img/intro-carousel/1.jpg" style="height:100%; width:100%;"  />
                    <div class="carousel-container">
                        <div class="container">
                            <h2 class="animate__animated animate__fadeInDown">Description</h2>
                            <p class="animate__animated animate__fadeInUp">A personal portfolio is a collection of educational, information, projects, skills, and documentation that relates specifically to an understanding of a user's individual vision needs.</p>
                            <a href="/API/UserAPI/CreatePortfolio" class="btn-get-started scrollto animate__animated animate__fadeInUp">Build your Portfolio</a>
                        </div>
                    </div>
                </div>

                <div class="carousel-item">
                    <img src="~/assets/img/intro-carousel/2.jpg" style="height:100%; width:100%;" />
                    <div class="carousel-container">
                        <div class="container">
                            <h2 class="~/animate__animated animate__fadeInDown">Description</h2>
                            <p class="~/animate__animated animate__fadeInUp">You can easily view samples of your work, details about yourself, projects completed in the past, and explain to those who are better off why they should use your services for their benefits.</p>
                            <a href="/API/UserAPI/CreatePortfolio" class="btn-get-started scrollto animate__animated animate__fadeInUp">Build your Portfolio</a>
                        </div>
                    </div>
                </div>

                <div class="carousel-item">
                    <img src="~/assets/img/intro-carousel/3.jpg" style="height:100%; width:100%;" />
                    <div class="carousel-container">
                        <div class="container">
                            <h2 class="animate__animated animate__fadeInDown">Description</h2>
                            <p class="animate__animated animate__fadeInUp">Portfolio entries can be based on activities from the university to adult life. On this site you can create your personal portfolio.</p>
                            <a href="/API/UserAPI/CreatePortfolio" class="btn-get-started scrollto animate__animated animate__fadeInUp">Build your Portfolio</a>
                        </div>
                    </div>
                </div>

            </div>

            <a class="carousel-control-prev" href="#introCarousel" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon ion-chevron-left" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>

            <a class="carousel-control-next" href="#introCarousel" role="button" data-slide="next">
                <span class="carousel-control-next-icon ion-chevron-right" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>

        </div>
    </div>
</section><!-- End Intro Section -->
